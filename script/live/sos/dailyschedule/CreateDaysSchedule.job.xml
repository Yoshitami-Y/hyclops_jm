<?xml version="1.0" encoding="ISO-8859-1"?>

<job title="Creating a DailySchedule depending on actual Runtimes" order="no">
    <description>
        <include file="jobs/JobSchedulerCreateDailySchedule.xml"/>
    </description>
    <params>
    		<param name="SchedulerHostName" value="localhost"/>
        <param name="dayOffset" value="10"/>
    </params>
    <script language="java"
            java_class="com.sos.dailyschedule.job.CreateDailyScheduleJSAdapterClass"
            java_class_path=""/>
    <run_time>
        <period  single_start="00:00:01"/>
    </run_time>
</job>
