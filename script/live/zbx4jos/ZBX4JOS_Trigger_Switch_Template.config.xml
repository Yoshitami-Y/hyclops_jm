<?xml version="1.0" encoding="ISO-8859-1"?>

<?xml-stylesheet type="text/xsl" href="scheduler_configuration_documentation.xsl"?> 
<settings>
    <job_chain name="ZBX4JOS_Trigger_Switch_Template">
        <note language="de"/>

        <note language="en"/>

        <order>
            <params>
                <param name="ZBX4JOS_HOSTID"
                       value="10106"/>

                <note language="de">
                    <div xmlns="http://www.w3.org/1999/xhtml"/>
                </note>

                <note language="en"/>

                <param name="ZBX4JOS_DESP"
                       value="&quot;Lack of available memory on server&quot;"/>

                <note language="de">
                    <div xmlns="http://www.w3.org/1999/xhtml"/>
                </note>

                <note language="en"/>

                <param name="ZBX4JOS_RULE"
                       value="&quot;{jos_server:vm.memory.size[available].last(0)}&lt;5M&quot;"/>

                <note language="de">
                    <div xmlns="http://www.w3.org/1999/xhtml"/>
                </note>

                <note language="en"/>

                <param name="Z4J_HOME"
                       value="&quot;/home/OSSLUSER/zbx4jos&quot;"/>

                <note language="de">
                    <div xmlns="http://www.w3.org/1999/xhtml"/>
                </note>

                <note language="en"/>
            </params>

            <process state="Trigger_ret">
                <note language="de"/>

                <note language="en"/>

                <params/>
            </process>
        </order>
    </job_chain>
</settings>
